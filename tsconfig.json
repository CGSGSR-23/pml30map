{

    "compilerOptions": {
      "target": "ES6",
      "lib": [
        "dom",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "noEmit": true,
      "jsx": "react-jsx"
    },
    "include": [
      "."
    ]
}